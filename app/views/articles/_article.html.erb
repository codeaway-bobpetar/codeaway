<% obj.each do |article| %>
  <div class="list-group-item">
    <h4 class="list-group-item-heading" align="center"><%= article.title %></h4>
    <p class="list-group-item-text" align="center"><%= article.description %></p>
    <% if logged_in? %>
      <%= link_to "Edit", edit_article_path(article) %>|<%= link_to "Show", article_path(article) %><div class="pull-sm-right"><%= link_to "Delete", article_path(article), method: :delete, data: {confirm: "Are you sure? "} %></div>
    <% end %>
  </div>
<% end %>